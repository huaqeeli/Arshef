               </VBox.margin>
            </HBox>
            <StackPane fx:id="stackPane" VBox.vgrow="ALWAYS">
               <VBox.margin>
                  <Insets bottom="5.0" left="5.0" right="5.0" />
               </VBox.margin>
               <children>
                  <ScrollPane fitToHeight="true" fitToWidth="true" prefWidth="3000.0">
                     <content>
                        <VBox fx:id="vbox" />
                     </content>
                  </ScrollPane>
               </children>
            </StackPane>
         </children>
      </VBox>
   </children>
</AnchorPane>
